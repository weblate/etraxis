{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": [
            "error", 4, {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error", "unix"
        ],
        "quotes": [
            "error", "double"
        ],
        "semi": [
            "error", "always"
        ],
        "vue/multi-word-component-names": [
            "error", {
                "ignores": [
                    "column",
                    "datatable",
                    "tab",
                    "tabs"
                ]
            }
        ]
    }
}
